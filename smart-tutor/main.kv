        icon:   "login"
        pos_hint:{'center_x':0.2,'center_y':0.1}
        on_press:root.manager.current = 'reg'
<RegisterScreen>:
    name:'reg'
    namee: namee
    email: email
    password: passw

    FloatLayout:
        cols:1

        FloatLayout:
            size: root.width, root.height/2
            AsyncImage:
                source: 'login4.jpg'
                allow_stretch: True
                keep_ratio: False
                size_hint: 1, 1.8
                pos_hint: {'center_x':.5, 'center_y': .9}
            Label:
                size_hint: 0.2,0.3
                color:0,0,0,1
                pos_hint: {"x":0.1, "top":0.8}
                text: "Name: "
                font_size: '20sp'

            TextInput:
                pos_hint: {"x":0.3, "top":0.68}
                size_hint: 0.6, 0.07
                background_color:105,108,102,101
                id: namee
                multiline: False
                font_size: '20sp'
            Label:
                size_hint: 0.2,0.3
                color:0,0,0,1
                pos_hint: {"x":0.1, "top":0.7}
                text: "Email: "
                font_size: '20sp'

            TextInput:
                pos_hint: {"x":0.3, "top":0.58}
                background_color:105,108,102,101
                size_hint: 0.6, 0.07
                id: email
                multiline: False
                font_size: '20sp'

            Label:
                size_hint: 0.2,0.3
                color:0,0,0,1
                pos_hint: {"x":0.05, "top":0.6}
                text: "Password: "
                font_size: '20sp'

            TextInput:
                pos_hint: {"x":0.3, "top":0.49}
                background_color:105,108,102,101
                size_hint: 0.6, 0.07
                id: passw
                multiline: False
                password: True
                font_size: '20sp'

            Button:
                pos_hint:{"x":0.38,"y":0.3}
                color:0,0,0,1
                background_color:1,0,0,0
                size_hint: 0.3, 0.08
                text: "Submit"
                font_size: '25sp'
                on_release:
                    root.submit()



        MDFillRoundFlatButton:
            text:'Already have an account??\nLogin'

            pos_hint:{'center_x':0.55,'center_y':0.1}
            on_press:root.manager.current = 'login'


        MDLabel:
            text:'Register Here'
            font_style:'H4'
            pos_hint:{'center_x':0.8,'center_y':0.77}
